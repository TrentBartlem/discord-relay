{
  "private": true,
  "name": "discord-relay",
  "version": "2.1.1.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "bump-major": "devvit upload --bump major",
    "bump-minor": "devvit upload --bump minor",
    "bump-patch": "devvit upload --bump patch",
    "bump-prerelease": "devvit upload --bump prerelease",
    "publish-major": "npm run bump-major && devvit publish",
    "publish-minor": "npm run bump-minor && devvit publish",
    "publish-patch": "npm run bump-patch && devvit publish",
    "publish-prerelease": "npm run bump-prerelease && devvit publish"
  },
  "dependencies": {
    "@devvit/public-api": "^0.10.25",
    "devvit": "^0.10.25"
  },
  "devDependencies": {
    "@devvit/tsconfig": "0.10.12",
    "typescript": "4.9.3",
    "vitest": "0.31.1"
  }
}
